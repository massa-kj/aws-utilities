# QuickSight Service Configuration
# Service-specific settings for QuickSight operations

# QuickSight API Configuration
QUICKSIGHT_API_VERSION="2018-04-01"
QUICKSIGHT_NAMESPACE="default"

# Target Resources
TARGET_ANALYSES=()
TARGET_DATASETS=()

# Backup Configuration
BACKUP_DIR_PREFIX="quicksight-backup"
INCLUDE_PERMISSIONS="true"
INCLUDE_DEFINITIONS="true"

# Operation Settings
MAX_CONCURRENT_OPERATIONS="5"
RETRY_COUNT="3"
RETRY_DELAY="2"

# Feature Flags
ENABLE_CONTENT_DIFF="true"
ENABLE_BATCH_OPERATIONS="true"
ENABLE_VALIDATION="true"

# Performance Tuning
QUICKSIGHT_RATE_LIMIT="10"  # requests per second
BATCH_SIZE="50"
